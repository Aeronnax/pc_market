				"properties": {
					"by": {
						"type": "string",
						"enum": [
							"id",
							"name",
							"description",
							"price",
							"category"
						],
						"nullable": false,
						"description": "Поле для сортировки"
					},
					"order": {
						"type": "string",
						"enum": [
							"asc",
							"desc"
						],
						"description": "Порядок сортировки"
					}
				},
				"required": [
					"by",
					"order"
				],
				"type": "object",
				"additionalProperties": false
			},
