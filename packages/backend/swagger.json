		"schemas": {
			"UsersDTO": {
				"description": "Стандартная DTO пользователя",
				"properties": {
					"id": {
						"type": "integer",
						"format": "int32",
						"example": 1
					},
					"firstName": {
						"type": "string",
						"example": "Иван"
					},
					"lastName": {
						"type": "string",
						"example": "Иванов"
					},
					"email": {
						"type": "string",
						"example": "ivan.ivanov@gmail.com",
						"pattern": "^(.+)@(.+)$"
					}
				},
				"required": [
					"id",
					"firstName",
					"lastName",
					"email"
				],
				"type": "object",
				"additionalProperties": false
			},
			"NotFoundErrorJSON": {
				"properties": {
					"message": {
						"type": "string"
					}
				},
				"required": [
					"message"
				],
				"type": "object",
				"additionalProperties": false
			},
			"FieldErrors": {
				"properties": {},
				"type": "object",
