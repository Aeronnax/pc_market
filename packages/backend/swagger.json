					"categoryId": {
						"type": "number",
						"format": "double",
						"description": "ID категории для фильтрации"
					},
					"minPrice": {
						"type": "number",
						"format": "double",
						"description": "Минимальная цена"
					},
					"maxPrice": {
						"type": "number",
						"format": "double",
						"description": "Максимальная цена"
					},
					"sort": {
						"$ref": "#/components/schemas/SortDTO_ProductDTO_"
					}
				},
				"required": [
					"take",
					"skip"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
