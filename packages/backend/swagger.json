				"type": "object",
				"additionalProperties": false
			},
			"ProductDTO": {
				"description": "Стандартный DTO для продукта",
				"properties": {
					"id": {
						"type": "integer",
						"format": "int32",
						"example": 1
					},
					"name": {
						"type": "string",
						"example": "Intel-core i7"
					},
					"description": {
						"type": "string",
						"example": "Горячая штучка"
					},
					"price": {
						"type": "number",
						"format": "double",
						"example": 12554.8
					},
