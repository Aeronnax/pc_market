{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/**/*.controller.ts"],
  "spec": {
    "outputDirectory": ".",
    "specVersion": 3
  },
  "routes": {
    "routesFileName": "routes.ts",
    "routesDir": "src",
    "esm": true,
    "middleware": "express"
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"]
    }
  }
}
